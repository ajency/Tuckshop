var dpi=0,getScreenDpi=function(){return 0==dpi|null==dpi&&(dpi=Ti.Platform.displayCaps.dpi),dpi};exports.getScreenDpi=getScreenDpi;var width=0,getScreenWidth=function(){return width=Ti.Platform.displayCaps.platformWidth};exports.getScreenWidth=getScreenWidth;var height=0,getScreenHeight=function(){return height=Ti.Platform.displayCaps.platformHeight};exports.getScreenHeight=getScreenHeight;var convertDptoPx=function(e){return e*(getScreenDpi()/160)};exports.convertDptoPx=convertDptoPx;var convertPxtoDp=function(e){return e/(getScreenDpi()/160)};exports.convertPxtoDp=convertPxtoDp;var osVersion=Ti.Platform.version,isiOS7Plus=function(){var e=osVersion,t=0;null!=e&&(t=osVersion.split("."));var i=parseInt(t[0],10);return i>=7?!0:!1};exports.isiOS7Plus=isiOS7Plus;var isPortrait=function(){var e=!1;return(1==Titanium.UI.orientation||2==Titanium.UI.orientation)&&(e=!0),e};exports.isPortrait=isPortrait;