var Q=require("/q"),cachedImageView=function(e,t){var i=Q.defer();Ti.API.info("Started");var s=t.split("/");s=s[s.length-1];var r=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e,s);if(r.exists())localPath=r.nativePath;else{var o=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e);o.exists()||o.createDirectory();var a=Ti.Network.createHTTPClient();a.setTimeout(15e3),a.onload=function(){if(200==a.status){r.write(a.responseData),Ti.API.info("file path"),Ti.API.info(r.nativePath);var e={response:r.nativePath};i.resolve(e)}},a.open("GET",t),a.send()}};exports.cachedImageView=cachedImageView;